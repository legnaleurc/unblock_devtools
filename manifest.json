{

  "manifest_version": 2,

  "name": "Unblock DevTools",
  "description": "Remove DevTools interfering scripts.",

  "version": "1.0",

  "applications": {
    "gecko": {
      "id": "unblockdevtools@wcpan",
      "strict_min_version": "57.0"
    }
  },

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],

  "background": {
    "scripts": [
      "chrome/main.js"
    ]
  }

}
